<h1>Welcome to my product page!</h1>

<% if @organs.blank? %>
  <h3> There are no items that match "<%= params[:search] %>".<h3>
<% end %>
<% @organs.each do |organ| %>
  <div class="col-md-4 product-index-single">
    <div class="well">
      <a href="/organs/<%= organ.id %>"><h3><%= organ.name %></h3></a>
      <img src="<%= organ.images.first.url%>">




    </div>
  </div>
<% end %>